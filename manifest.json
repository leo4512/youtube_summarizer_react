{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "YouTube Summarizer",
    "description": "YouTube Summarizer",
    "permissions": ["storage"],
    "action": {
        "default_popup": "js/index.html",
        "default_title": "YouTube Summarizer"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}